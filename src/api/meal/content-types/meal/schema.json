{
  "kind": "collectionType",
  "collectionName": "meals",
  "info": {
    "singularName": "meal",
    "pluralName": "meals",
    "displayName": "Meal",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "minLength": 4,
      "maxLength": 100,
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 0.1
    },
    "discount_percentage": {
      "type": "decimal",
      "required": true,
      "min": 0,
      "max": 100
    },
    "store": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::store.store",
      "inversedBy": "meals"
    },
    "available_to_purchase": {
      "type": "boolean",
      "required": false,
      "default": true
    },
    "recurring_stock": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "meal_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::meal-category.meal-category"
    }
  }
}
